<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd">

<mapper namespace="com.warehouse.mapper.PurInboundMapper">
    <insert id="addPurInboundInfo" parameterType="com.warehouse.pojo.PurInbound">
        insert into purInbound(pui_id, pui_supId, pui_contacts, pui_phone,inb_id)values(
            #{puiId}, #{puiSupId}, #{puiContacts}, #{puiPhone},#{inbId}
        );
    </insert>
</mapper>